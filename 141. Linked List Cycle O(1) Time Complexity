/**
 * Definition for singly-linked list.
 * function ListNode(val) {
 *     this.val = val;
 *     this.next = null;
 * }
 */

/**
 * @param {ListNode} head
 * @return {boolean}
 */
var hasCycle = function(head) {
    let loopHead = head;
    let p = head;

    while(p != null && p.next != null){
        if(p.next === loopHead)
            return true;
        else{
            p = p.next.next;
            loopHead = loopHead.next;
        }
    }

    return false

};
