/**
 * @param {number[]} height
 * @return {number}
 */
var maxArea = function(height) {
    let max = 0;
    let p1 = 0;
    let p2 = height.length - 1;

    let currHeight 

    while(p1<p2){
        height[p1] >= height[p2] ? currHeight = height[p2] : currHeight = height[p1];

        let area = currHeight * (p2 - p1);
        if(max <= area) max = area;

        if(height[p1] >= height[p2]) p2--;
        else p1++

        // console.log(max, p1, p2);
    }

    return max;
};
